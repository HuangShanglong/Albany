        <Parameter name="Loading Case Name" type="string" value="hydrostatic"/>
        <Parameter name="Number of Steps" type="int" value="10"/>
        <Parameter name="Step Size" type="double" value="0.02"/>
        <Parameter name="Output File Name" type="string" value="Gurson-hydrostatic.exo"/>
