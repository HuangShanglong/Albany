<ParameterList>
  <ParameterList name="Problem">
    <Parameter name="Solution Method" type="string" value="ATO Problem" />
    <Parameter name="Name" type="string" value="Elasticity 2D" />

    <Parameter name="Verbose Output" type="bool" value="1" />
    <Parameter name="Phalanx Graph Visualization Detail" type="int" value="1"/>

    <Parameter name="Maximum Optimization Iterations" type="int" value="1" />
    <Parameter name="Iterative Convergence Tolerance" type="double" value="1e-9" />

    <ParameterList name="Elasticity 2D">    
  
      <ParameterList name="Dirichlet BCs">
        <Parameter name="DBC on NS nodelist_1 for DOF Y" type="double" value="0.0"/>
        <Parameter name="DBC on NS nodelist_2 for DOF X" type="double" value="0.0"/>
      </ParameterList>

      <ParameterList name="Elastic Modulus">
        <Parameter name="Elastic Modulus Type" type="string" value="Constant"/>
        <Parameter name="Value" type="double" value="206900.0"/>
      </ParameterList>

      <ParameterList name="Poissons Ratio">
        <Parameter name="Poissons Ratio Type" type="string" value="Constant"/>
        <Parameter name="Value" type="double" value="0.29"/>
      </ParameterList>

    </ParameterList>  <!-- end of Physics Problem Specification -->
  </ParameterList> <!-- end of Problem -->

  <ParameterList name="Discretization">
    <Parameter name="Method" type="string" value="Ioss"/>
    <Parameter name="Exodus Input File Name" type="string" value="ato_mesh.exo"/>
    <Parameter name="Exodus Output File Name" type="string" value="ato_out.exo"/>
  </ParameterList>

</ParameterList>
